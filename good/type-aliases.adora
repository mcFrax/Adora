# aliasy mogą być definiowane zarówno przed,
# jak i po aliasowanym typie

type Foo1 = Foo2
type Foo2 = Foo
struct Foo():
    Int foo

type Foo3 = Foo1

var foo = Foo(foo=0)
print foo.[foo]

foo = Foo1(foo=1)
print foo.[foo]

foo = Foo2(foo=2)
print foo.[foo]

foo = Foo3(foo=3)
print foo.[foo]

# EXPECTED OUTPUT #
# 0
# 1
# 2
# 3
