# poÅ›rednia implementacja

class Base():
    writable Int wr
    Int ro

class Derived(Base):
    pass

struct Impl():
    Int wr_
    Int wr:
        get:
            return self.[wr_]
        set:
            assert value > 0
            self.[wr_] = value

    implement Derived:
        Int ro:
            get:
                return 1

fn () -> Base:
    return Impl(wr_=1) as Base

fn () -> Derived:
    return Impl(wr_=1) as Derived
